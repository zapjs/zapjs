    "!internal/*/dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist internal/*/dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "inquirer": "^9.2.12",
    "ws": "^8.16.0",
    "vite": "^5.0.0",
    "ora": "^6.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.7",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3"
