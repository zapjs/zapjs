{
  "name": "@zap-js/client",
  "version": "0.1.0",
  "description": "ZapJS client-side React framework with routing and development tools",
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js"
  },
  "bin": {
    "zap": "./internal/cli/src/index.js"
  },
  "files": [
    "index.js",
    "src",
    "internal",
    "!internal/*/src/**/*.ts",
