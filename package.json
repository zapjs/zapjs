{
  "name": "zapjs",
  "version": "0.1.0",
  "private": true,
  "description": "High-performance fullstack React framework powered by Rust",
  "workspaces": [
    "packages/server",
    "packages/client"
  ],
  "scripts": {
    "build": "cargo build --release --workspace",
    "test": "cargo test --workspace",
    "clean": "rm -rf target packages/*/dist packages/*/internal/*/dist",
    "format": "prettier --write 'packages/**/*.{js,json}'"
  },
  "keywords": [
    "react",
    "rust",
    "framework",
    "typescript",
    "fullstack",
    "server-functions",
    "performance",
    "web-server",
    "router",
    "file-routing",
    "next",
    "remix",
    "vite",
    "ssr",
    "ssg",
    "middleware",
    "rpc",
    "websocket",
    "hot-reload",
    "hmr"
  ],
  "author": "saint0x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saint0x/zapjs.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  }
}